tosca_definitions_version: tosca_simple_yaml_1_3

node_types:
  radon.nodes.OpenWorkload:
    derived_from: radon.nodes.Workload
    metadata:
      targetNamespace: "radon.nodes"
      abstract: "false"
      final: "false"
    properties:
      interarrival_time:
        type: radon.datatypes.TimeRandomVariable
      type:
        type: string
        default: "open-workload"
        constraints:
          - equal: open-workload
